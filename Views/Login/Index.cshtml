@model Login;
@{
    Layout = "~/Views/Shared/_Layout-login.cshtml";
}
<form method="post">
    <fieldset>
        <div class="form-group">
            <label for="exampleInputEmail1" class="form-label mt-4">Email address</label>
            <input type="email" class="form-control" asp-for="Email" aria-describedby="emailHelp" placeholder="Enter email" fdprocessedid="agkfwr">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1" class="form-label mt-4">Password</label>
            <input type="password" class="form-control" asp-for="Password" placeholder="Password" autocomplete="off" fdprocessedid="gfetk">
        </div>

        <button type="submit" class="btn btn-primary" fdprocessedid="syc4qo" asp-controller="Login" asp-action="AuthenticateUser">Submit</button>
    </fieldset>
</form>
